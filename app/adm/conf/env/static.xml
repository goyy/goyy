<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<asset name="goyy">
				<enable>true</enable>
				<dir>/app/assets</dir>
				<url>/assets</url>
			</asset>
			<static name="goyy">
				<enable>true</enable>
				<dir>static</dir>
				<url>/static</url>
			</static>
			<developer name="goyy">
				<enable>true</enable>
				<dir>/app/assets/gydev</dir>
				<url>/gydev</url>
			</developer>
			<operation name="goyy">
				<enable>true</enable>
				<dir>/app/assets/gyopr</dir>
				<url>/gyopr</url>
			</operation>
		</environment>
		<environment id="test">
			<asset name="goyy">
				<enable>false</enable>
				<dir>/app/assets</dir>
				<url>http://127.0.0.1:9099</url>
			</asset>
			<static name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gyadm</dir>
				<url>http://127.0.0.1:9099/gyadm</url>
			</static>
			<developer name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gydev</dir>
				<url>http://127.0.0.1:9099/gydev</url>
			</developer>
			<operation name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gyopr</dir>
				<url>http://127.0.0.1:9099/gyopr</url>
			</operation>
		</environment>
		<environment id="production">
			<asset name="goyy">
				<enable>false</enable>
				<dir>/app/assets</dir>
				<url>http://assets.goyy.org</url>
			</asset>
			<static name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gyadm</dir>
				<url>http://assets.goyy.org/gyadmyadm</url>
			</static>
			<developer name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gydev</dir>
				<url>http://assets.goyy.org/gydev</url>
			</developer>
			<operation name="goyy">
				<enable>false</enable>
				<dir>/app/assets/gyopr</dir>
				<url>http://assets.goyy.org/gyopr</url>
			</operation>
		</environment>
	</environments>
</configuration>
