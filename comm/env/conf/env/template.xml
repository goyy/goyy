<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<html name="env">
				<enable>true</enable>
				<reloaded>true</reloaded>
			</html>
			<template name="env">
				<enable>true</enable>
				<reloaded>true</reloaded>
			</template>
		</environment>
		<environment id="test">
			<html name="env">
				<enable>true</enable>
				<reloaded>false</reloaded>
			</html>
			<template name="env">
				<enable>true</enable>
				<reloaded>false</reloaded>
			</template>
		</environment>
		<environment id="production">
			<html name="env">
				<enable>true</enable>
				<reloaded>false</reloaded>
			</html>
			<template name="env">
				<enable>true</enable>
				<reloaded>false</reloaded>
			</template>
		</environment>
	</environments>
</configuration>
