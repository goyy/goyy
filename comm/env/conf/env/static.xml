<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<asset name="env">
				<enable>true</enable>
				<url>/assets</url>
				<dir>/app/assets</dir>
			</asset>
			<static name="env">
				<enable>true</enable>
				<url>/static</url>
				<dir>static</dir>
			</static>
			<developer name="env">
				<enable>true</enable>
				<url>/gydev</url>
				<dir>/app/assets/gydev</dir>
			</developer>
			<operation name="env">
				<enable>true</enable>
				<url>/gyopr</url>
				<dir>/app/assets/gyopr</dir>
			</operation>
		</environment>
		<environment id="test">
			<asset name="env">
				<enable>false</enable>
				<url>/assets</url>
				<dir>/app/assets</dir>
			</asset>
			<static name="env">
				<enable>false</enable>
				<url>/static</url>
				<dir>static</dir>
			</static>
			<developer name="env">
				<enable>false</enable>
				<url>/gydev</url>
				<dir>/app/assets/gydev</dir>
			</developer>
			<operation name="env">
				<enable>false</enable>
				<url>/gyopr</url>
				<dir>/app/assets/gyopr</dir>
			</operation>
		</environment>
		<environment id="production">
			<asset name="env">
				<enable>false</enable>
				<url>/assets</url>
				<dir>/app/assets</dir>
			</asset>
			<static name="env">
				<enable>false</enable>
				<url>/static</url>
				<dir>static</dir>
			</static>
			<developer name="env">
				<enable>false</enable>
				<url>/gydev</url>
				<dir>/app/assets/gydev</dir>
			</developer>
			<operation name="env">
				<enable>false</enable>
				<url>/gyopr</url>
				<dir>/app/assets/gyopr</dir>
			</operation>
		</environment>
	</environments>
</configuration>
