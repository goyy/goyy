<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<asset name="env">
				<enable>true</enable>
				<mappings url="/assets" dir="/app/assets"/>
			</asset>
			<static name="env">
				<enable>true</enable>
				<mappings url="/static" dir="static"/>
			</static>
			<developer name="env">
				<enable>true</enable>
				<mappings url="/gydev" dir="/app/assets/gydev"/>
			</developer>
			<operation name="env">
				<enable>true</enable>
				<mappings url="/gyopr" dir="/app/assets/gyopr"/>
			</operation>
		</environment>
		<environment id="test">
			<asset name="env">
				<enable>false</enable>
				<mappings url="/assets" dir="/app/assets"/>
			</asset>
			<static name="env">
				<enable>false</enable>
				<mappings url="/static" dir="static"/>
			</static>
			<developer name="env">
				<enable>false</enable>
				<mappings url="/gydev" dir="/app/assets/gydev"/>
			</developer>
			<operation name="env">
				<enable>false</enable>
				<mappings url="/gyopr" dir="/app/assets/gyopr"/>
			</operation>
		</environment>
		<environment id="production">
			<asset name="env">
				<enable>false</enable>
				<mappings url="/assets" dir="/app/assets"/>
			</asset>
			<static name="env">
				<enable>false</enable>
				<mappings url="/static" dir="static"/>
			</static>
			<developer name="env">
				<enable>false</enable>
				<mappings url="/gydev" dir="/app/assets/gydev"/>
			</developer>
			<operation name="env">
				<enable>false</enable>
				<mappings url="/gyopr" dir="/app/assets/gyopr"/>
			</operation>
		</environment>
	</environments>
</configuration>
